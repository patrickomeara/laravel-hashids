{
    "name": "vinkla/hashids",
    "description": "A Hashids bridge for Laravel",
    "license": "MIT",
    "keywords": [
        "hashids",
        "laravel"
    ],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@doubledip.se"
        }
    ],
    "require": {
        "php": "^8.1",
        "graham-campbell/manager": "dev-l10-compatibility",
        "hashids/hashids": "^4.1",
        "illuminate/contracts": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0",
        "graham-campbell/testbench": "^5.7",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Vinkla\\Hashids\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Hashids\\": "tests/"
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/laravel-shift/Laravel-Manager.git"
    }],
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.0-dev"
        },
        "laravel": {
            "aliases": {
                "Hashids": "Vinkla\\Hashids\\Facades\\Hashids"
            },
            "providers": [
                "Vinkla\\Hashids\\HashidsServiceProvider"
            ]
        }
    }
}
